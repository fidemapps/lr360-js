{
  "name": "lr360-js",
  "version": "0.1.0",
  "description": "Node module to access Lune Rouge 360 API.",
  "keywords": [
    "lune rouge 360",
    "lune",
    "rouge",
    "lr360"
  ],
  "author": "Luis Doebbel <ldoebbel@lunerouge360.com>",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:fidemapps/lr360-js.git"
  },
  "bugs": {
    "email": "ldoebbel@lunerouge360.com",
    "url": "https://github.com/fidemapps/lr360-js/issues"
  },
  "engines" : {
    "node" : "4.2.3",
    "npm": "2.14.7"
  },
  "scripts": {
    "preinstall": "npm set progress=false",
    "test": "jscs src/**/*.js && jscs tests/**/*.js && NODE_ENV=test karma start",
    "test-coverage": "NODE_ENV=test karma start --coverage",
    "build": "browserify -t babelify src/index.js | uglifyjs -cm > dist/lr360.js",
    "watch": "watchify -t babelify src/index.js -o 'uglifyjs -cm > dist/lr360.js'"
  },
  "licence": "MIT",
  "dependencies": {
    "browser-request": "^0.3.3",
    "lodash": "^4.0.0",
    "lodash.assign": "^4.0.1",
    "lodash.merge": "^4.0.3",
    "superagent": "^1.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-istanbul": "^0.2.1",
    "chai": "^3.4.1",
    "isparta": "^4.0.0",
    "jscs": "^2.9.0",
    "karma": "^0.13.19",
    "karma-browserify": "^4.4.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-sinon": "^1.0.4",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "uglify-js": "^2.6.1",
    "watchify": "^3.7.0"
  }
}
